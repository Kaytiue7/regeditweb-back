generator client {
  provider = "prisma-client-js"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MainPageSection {
  id           String @id @default(uuid()) // Otomatik benzersiz ID
  sectionName  String @unique // Bölüm adı (main-begining, main-middle-first, vb.)
  content      Json   // JSON verisini depolamak için Json tipi
}
